{
    "name": "bachata-science",
    "version": "1.0.0",
    "description": "A catalog of the most common bachata dance steps",
    "main": "index.js",
    "scripts": {
        "build": "npm run clean:build && webpack --mode production",
        "clean:build": "rm -rf dist",
        "clean:dependencies": "rm -rf node_modules/ package-lock.json",
        "prettier:check": "prettier --list-different \"src/**/*.{ts,tsx}\"",
        "prettier:write": "prettier --write \"src/**/*.{ts,tsx}\"",
        "start": "node index.js",
        "start:dev": "webpack-dev-server --open --mode development"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/L3bowski/bachata-science"
    },
    "keywords": [
        "bachata",
        "science"
    ],
    "author": "Carles Capellas <capellas.carles@gmail.com>",
    "license": "MIT",
    "dependencies": {
        "express": "4.17.1",
        "react": "16.12.0",
        "react-dom": "16.12.0",
        "react-router-dom": "5.1.2"
    },
    "devDependencies": {
        "@types/react": "16.9.17",
        "@types/react-dom": "16.9.4",
        "@types/react-router-dom": "5.1.3",
        "awesome-typescript-loader": "5.2.1",
        "css-loader": "3.4.1",
        "html-loader": "0.5.5",
        "html-webpack-plugin": "3.2.0",
        "node-sass": "4.13.0",
        "prettier": "1.19.1",
        "sass-loader": "8.0.0",
        "style-loader": "1.1.2",
        "typescript": "3.7.4",
        "webpack": "4.41.5",
        "webpack-cli": "3.3.10",
        "webpack-dev-server": "3.10.1"
    }
}
